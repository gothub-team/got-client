{
    "name": "@gothub-team/got",
    "version": "0.21.21-3",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "build:util": "yarn workspace @gothub-team/got-util build",
        "build:core": "yarn workspace @gothub-team/got-core build",
        "build:store": "yarn workspace @gothub-team/got-store build",
        "build:react": "yarn workspace @gothub-team/got-react build",
        "build": "yarn build:util; yarn build:core; yarn build:store; yarn build:react;",
        "test:util": "yarn workspace @gothub-team/got-util test",
        "test:core": "yarn workspace @gothub-team/got-core test",
        "test:store": "yarn workspace @gothub-team/got-store test",
        "test:react": "yarn workspace @gothub-team/got-react test",
        "test:react:fn": "yarn workspace @gothub-team/got-react test:fn '$1'",
        "test": "yarn test:store; yarn test:util; yarn test:react",
        "lint": "eslint ./packages",
        "publish:util": "yarn workspace @gothub-team/got-util npm publish --access public",
        "publish:core": "yarn workspace @gothub-team/got-core npm publish --access public",
        "publish:store": "yarn workspace @gothub-team/got-store npm publish --access public",
        "publish:react": "yarn workspace @gothub-team/got-react npm publish --access public",
        "publish:all": "yarn publish:util; yarn publish:core; yarn publish:store; yarn publish:react;",
        "publish:util:beta": "yarn workspace @gothub-team/got-util npm publish --access public --tag beta",
        "publish:core:beta": "yarn workspace @gothub-team/got-core npm publish --access public --tag beta",
        "publish:store:beta": "yarn workspace @gothub-team/got-store npm publish --access public --tag beta",
        "publish:react:beta": "yarn workspace @gothub-team/got-react npm publish --access public --tag beta",
        "publish:all:beta": "yarn publish:util:beta; yarn publish:core:beta; yarn publish:store:beta; yarn publish:react:beta;",
        "patch:util": "yarn workspace @gothub-team/got-util version patch",
        "patch:core": "yarn workspace @gothub-team/got-core version patch",
        "patch:store": "yarn workspace @gothub-team/got-store version patch",
        "patch:react": "yarn workspace @gothub-team/got-react version patch",
        "prerelease:util": "npm version prerelease --workspace @gothub-team/got-util --git-tag-version false",
        "prerelease:core": "npm version prerelease --workspace @gothub-team/got-core --git-tag-version false",
        "prerelease:store": "npm version prerelease --workspace @gothub-team/got-store --git-tag-version false",
        "prerelease:react": "npm version prerelease --workspace @gothub-team/got-react --git-tag-version false",
        "bump:patch": "yarn patch:util; yarn patch:core; yarn patch:store; yarn patch:react; yarn version patch; yarn tag:version",
        "bump:prerelease": "yarn prerelease:util; yarn prerelease:core; yarn prerelease:store; yarn prerelease:react; npm version prerelease --git-tag-version false; yarn tag:version",
        "tag:version": "VERSION=$(jq -r '.version' ./package.json); git commit -am \"v$VERSION\"; git tag -a \"v$VERSION\" -m \"v$VERSION\""
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.18.2",
        "@babel/node": "^7.17.10",
        "@types/jest": "^27.5.1",
        "eslint": "^8.17.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "4.0.3"
    },
    "packageManager": "yarn@3.2.1",
    "stableVersion": "0.21.20"
}
