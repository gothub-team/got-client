{
    "name": "@gothub-team/got-store",
    "version": "0.21.24",
    "description": "Functions to interact with local (in-memory) got graphs and sync them with remote got providers.",
    "main": "dist/cjs/index.js",
    "types": "src/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/gothub-team/got.git",
        "directory": "packages/got-store"
    },
    "author": "GotHub UG",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/gothub-team/got/issues"
    },
    "homepage": "https://gothub.io/",
    "files": [
        "dist",
        "src",
        "!src/tests"
    ],
    "engines": {
        "node": ">=17.0.0"
    },
    "dependencies": {
        "@gothub-team/got-core": "workspace:*",
        "@gothub-team/got-util": "workspace:*",
        "ramda": "^0.28.0",
        "ramda-adjunct": "^3.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.17.10",
        "@babel/preset-env": "^7.17.10",
        "@gothub-team/got-api": "^0.1.20",
        "babel-jest": "^28.1.0",
        "babel-loader": "^8.2.5",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "jest": "^28.1.0",
        "uuid": "^8.3.2",
        "webpack": "^5.72.1",
        "webpack-cli": "4.9.2"
    },
    "scripts": {
        "test": "jest ./src --passWithNoTests",
        "test:fn": "jest ./src/tests/$1.spec.js --passWithNoTests",
        "build": "webpack --config webpack.config.js; babel src -d dist/cjs --ignore 'src/**/*.spec.js','src/tests'"
    },
    "jest": {
        "moduleFileExtensions": [
            "js"
        ],
        "rootDir": "./",
        "testRegex": ".*?(?=\\.spec).*?\\.js",
        "transform": {
            "\\.js?$": "babel-jest"
        }
    }
}
