{
    "name": "@gothub-team/test-got-react",
    "version": "0.21.21-3",
    "description": "Tests for the @gothub-team/got-react package",
    "main": "dist/cjs/index.js",
    "types": "src/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/gothub-team/got.git",
        "directory": "packages/got-react"
    },
    "author": "GotHub UG",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/gothub-team/got/issues"
    },
    "homepage": "https://gothub.io/",
    "files": [
        "dist",
        "src",
        "!src/tests"
    ],
    "engines": {
        "node": ">=17.0.0"
    },
    "dependencies": {
        "@gothub-team/got-react": "0.21.20",
        "@gothub-team/got-store": "workspace:*",
        "@gothub-team/got-util": "workspace:*",
        "ramda": "^0.28.0",
        "ramda-adjunct": "^3.1.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "7.2.3",
        "react-router-dom": "^6.3.0",
        "react-scripts": "5.0.1",
        "redux": "^4.2.0",
        "redux-logger": "^3.0.6",
        "unfetch": "^4.2.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.16.4",
        "@testing-library/react": "13.3.0",
        "jest": "27.0.0"
    },
    "scripts": {
        "test": "jest ./src --passWithNoTests",
        "build": "webpack --config webpack.config.js; babel src -d dist/cjs --ignore 'src/**/*.spec.js','src/tests'"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "jsx"
        ],
        "rootDir": "./",
        "testRegex": ".*?(?=\\.spec).*?\\.js",
        "transform": {
            "\\.js?$": "babel-jest",
            "\\.jsx?$": "babel-jest"
        },
        "testEnvironment": "jsdom"
    },
    "stableVersion": "0.21.18"
}
